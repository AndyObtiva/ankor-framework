<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Text?>

<Tab fx:id="tab" closable="true" xmlns:fx="http://javafx.com/fxml" fx:controller="at.irian.ankor.sample1.fxclient.animal.AnimalDetailTabController">
  <VBox prefHeight="350.0" prefWidth="519.0" spacing="10.0">
    <GridPane alignment="TOP_LEFT" hgap="10.0" vgap="5.0">
      <Label text="Name" GridPane.columnIndex="0" GridPane.rowIndex="0" />
      <TextField fx:id="name" prefWidth="208.5" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      <Text fx:id="nameStatus" fill="#cc3100" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
      <Label text="Type" GridPane.columnIndex="0" GridPane.rowIndex="1" />
      <ComboBox fx:id="type" prefWidth="209.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="Family" GridPane.columnIndex="0" GridPane.rowIndex="2" />
      <ComboBox fx:id="family" prefWidth="209.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Button onAction="#save" text="Save" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    </GridPane>
    <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
  </VBox>
</Tab>
